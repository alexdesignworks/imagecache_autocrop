/**
 * @file
 * Install file for imagecache_autocrop.
 */

/**
 * Implements hook_enable().
 */
function imagecache_autocrop_enable() {
  cache_clear_all('imagecache_actions', 'cache');
}

/**
 * Implements hook_disable().
 */
function imagecache_autocrop_disable() {
  cache_clear_all('imagecache_actions', 'cache');
}
